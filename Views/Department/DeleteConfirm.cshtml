@model hospital_project.Models.DepartmentDto

@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>Are you sure you want to delete this Department?</h2>
<a href="/Department/List">Cancel</a>
<h3>Name: @Model.department_name </h3>
<form action="/Department/Delete/@Model.department_id" method="POST">
    <input type="submit" value="Confirm Delete" />
</form>


