@model IEnumerable<hospital_project.Models.LabDto>

@{
    ViewBag.Title = "List";
}

<h2>Lab List</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.LabName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.LabName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.LabId }) |
            @Html.ActionLink("Details", "Details", new { id=item.LabId }) |
            @Html.ActionLink("Delete", "DeleteConfirm", new { id=item.LabId })
        </td>
    </tr>
}

</table>
